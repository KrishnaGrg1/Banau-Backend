{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm --filter @repo/db build && pnpm turbo run build --filter=banau",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}
