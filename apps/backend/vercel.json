{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm turbo run build --filter=@repo/backend",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}
